{
    "functions": {
        "api/**/*.js": {
            "memory": 128,
            "maxDuration": 10
        }
    },
    "rewrites": [
        {
            "source": "/get",
            "destination": "/api/get"
        },
        {
            "source": "/set",
            "destination": "/api/set"
        },
        {
            "source": "/log",
            "destination": "/api/log"
        },
        {
            "source": "/logs",
            "destination": "/api/logs"
        }
    ],
    "routes": [
        {
            "src": "/.*",
            "dest": "/SERVER/index.html"
        }
    ],
    "public": false
}